/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:      black ;
    text-color:             @fg;
	font:				 	"Fantasque Sans Mono 16";
}

window {
    width:      			400px;
    padding:    			20px;
    border:					1px;
    border-radius:          0px;
    border-color:           @bg1;
    location:               center;
    y-offset:               -2em;
}

entry {
    expand: 				true;
    text-color:				@fg;
}
